<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:sofa="http://sofastack.io/schema/sofaboot"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
            http://sofastack.io/schema/sofaboot http://sofastack.io/schema/sofaboot.xsd"
       default-autowire="byName">

    <!-- bolt引用 -->
    <sofa:reference jvm-first="false" id="boltSyncServiceReference" interface="kobe.been.zjq.rpc_service.IRemoteService">
        <sofa:binding.bolt/>
    </sofa:reference>

    <!-- rest引用 -->
    <sofa:reference jvm-first="false" id="restSyncServiceReference" interface="kobe.been.zjq.rpc_service.IRemoteServiceRest">
        <sofa:binding.rest/>
    </sofa:reference>

    <!-- dubbo引用 -->
    <sofa:reference jvm-first="false" id="dubboSyncServiceReference" interface="kobe.been.zjq.rpc_service.IRemoteService">
        <sofa:binding.dubbo/>
    </sofa:reference>
</beans>
